{
    "compilerOptions": {
        "target": "es2022",
        "lib": [
            "ES2023"
        ],
        "module": "Node16",
        "paths": {
            "@/*": [
                "./src/*"
            ],
        },
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    }
}